{
    "name": "portifolio",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": "^20.19.0 || >=22.12.0"
    },
    "scripts": {
        "dev": "vite",
        "optimize:svgs": "svgo -f src/assets/icons -o src/assets/icons/optimized",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --build",
        "lint": "eslint . --fix --cache",
        "deploy": "npm run build && gh-pages -d dist"
    },
    "dependencies": {
        "@primeuix/themes": "^2.0.1",
        "pinia": "^3.0.4",
        "primeicons": "^6.0.1",
        "primevue": "^4.5.0",
        "vue": "^3.5.25",
        "vue-router": "^4.6.3"
    },
    "devDependencies": {
        "@tsconfig/node24": "^24.0.3",
        "gh-pages": "^6.3.0",
        "@types/node": "^24.10.1",
        "@vitejs/plugin-vue": "^6.0.2",
        "@vue/eslint-config-typescript": "^14.6.0",
        "@vue/tsconfig": "^0.8.1",
        "eslint": "^9.39.1",
        "eslint-plugin-vue": "~10.5.1",
        "jiti": "^2.6.1",
        "npm-run-all2": "^8.0.4",
        "svgo": "^3.0.0",
        "typescript": "~5.9.0",
        "vite": "^7.2.4",
        "vite-plugin-vue-devtools": "^8.0.5",
        "vue-tsc": "^3.1.5"
    }
}